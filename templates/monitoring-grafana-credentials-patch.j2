spec:
  template:
    spec:
      containers:
        - env:
          - name: GF_SECURITY_ADMIN_USER
            valueFrom:
              secretKeyRef:
                key: user
                name: grafana-credentials
          - name: GF_SECURITY_ADMIN_PASSWORD
            valueFrom:
              secretKeyRef:
                key: password
                name: grafana-credentials
          name: grafana