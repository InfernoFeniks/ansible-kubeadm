spec:
  ports:
  - appProtocol: http
    name: http
    port: 80
    protocol: TCP
    targetPort: http
    nodePort: {{ kubeadm_ingress_nodeport_http | default('31080') }}
  - appProtocol: https
    name: https
    port: 443
    protocol: TCP
    targetPort: https
    nodePort: {{ kubeadm_ingress_nodeport_https | default('31443') }}